<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="56.0" prefWidth="784.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.what2watch.Controllers.RatedItemController">
   <children>
      <Label fx:id="titleLabel" prefHeight="56.0" prefWidth="468.0" text="Avatar: The Way of Water" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Impact" size="35.0" />
         </font>
      </Label>
      <Label fx:id="releaseDateLabel" prefHeight="56.0" prefWidth="171.0" text="2008" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Impact" size="35.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" style="-fx-opacity: 0;" text="Label" />
      <Label fx:id="ratingLabel" prefHeight="56.0" prefWidth="81.0" text="10" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Impact" size="35.0" />
         </font>
      </Label>
      <BorderPane fx:id="editButtonBorderPane" prefHeight="248.0" prefWidth="104.0">
         <center>
            <Button fx:id="editButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#editButtonAction" prefHeight="63.0" prefWidth="80.0" styleClass="transparent" stylesheets="@../css/Style.css" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../W2W Images/logos/WhiteEditButton.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets right="15.0" />
               </padding>
            </Button>
         </center>
      </BorderPane>
      <Label fx:id="restOfInfoLabel" layoutX="621.0" layoutY="10.0" style="-fx-opacity: 0;" text="Label">
         <font>
            <Font size="0.001" />
         </font>
      </Label>
   </children>
</HBox>
